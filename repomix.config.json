{
  "output": {
    "filePath": "repomix-output.json",
    "style": "json"
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "**/docs/**",
      "**/.storybook/**",
      "**/repomix-output.*",
      "**/stories/**"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
